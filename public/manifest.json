{
  "manifest_version": 3,
  "name": "Save Tabs",
  "version": "1.0",

  "background": {
    "service_worker": "background.js"
  },
  
  "action": {
    "default_popup": "popup.html"
  },
   "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  },
   "permissions": [
      "storage",
      "downloads",
      "tabs",
      "storage",
      "activeTab",
      "scripting",
      "identity",
      "identity.email"  
   ]
}
