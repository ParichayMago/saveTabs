{
  "manifest_version": 3,
  "name": "Hello World Extension",
  "version": "1.0",

  "background": {
    "service_worker": "background.js"
  },
  
  "action": {
    "default_popup": "popup.html"
  },
   "permissions": [
      "storage",
      "downloads",
      "tabs",
      "storage",
      "activeTab",
      "scripting",
      "identity",
      "identity.email"  
   ]
}
